<h1>Welcome</h1>
<p>This is a basic chat app</p>
<div class="list-room">
  <h4>List Rooms</h4>
  <ul class="list-group">
    <% @rooms.each do |room| %>
      <li class="list-group-item">
        <%= room.name %>
      </li>  
    <% end %>
  </ul>
</div>
<div class="divider"></div>

<div class="create-room-from">
  <h5>Create a room</h5>
  <%= render 'rooms/form' %>
</div>



